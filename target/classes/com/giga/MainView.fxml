<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/17"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.giga.controllers.MainController">
   <children>
      <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <tabs>
            <Tab text="Fire Test">
               <content>
                  <fx:include source="fireTestForm.fxml" fx:id="fireTestFormPane"/>
               </content>
            </Tab>
            <Tab text="Add Gun">
               <content>
                  <fx:include source="gunForm.fxml" fx:id="gunFormPane"/>
               </content>
            </Tab>
            <Tab text="Add Vehicle">
               <content>
                  <fx:include source="vehicleForm.fxml" fx:id="vehicleFormPane"/>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</VBox>
